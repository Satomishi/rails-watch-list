<div class="container">
  <div class="row justify-content-center">
    <div class="col-4">
      <h1>Add your new list</h1>
      <%= simple_form_for(@list) do |f| %>
        <%= f.input :name %>
        <%= f.input :photo, as: :file %>
        <%= f.submit class: 'btn btn-outline-primary' %>
      <% end %>
    </div>
  </div>
</div>
